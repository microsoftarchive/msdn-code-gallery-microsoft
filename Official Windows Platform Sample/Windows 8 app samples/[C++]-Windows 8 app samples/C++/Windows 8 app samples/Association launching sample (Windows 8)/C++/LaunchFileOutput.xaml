<Page
    x:Class="AssociationLaunching.LaunchFileOutput"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="300"
    d:DesignHeight="400">

    <Grid>
        <StackPanel x:Name="Output">
            <TextBlock Style="{StaticResource DescriptionTextStyle}" TextWrapping="Wrap">The default handler application for .png files is launched and displays in the foreground. This sample is moved to the background.</TextBlock>
            <TextBlock Style="{StaticResource DescriptionTextStyle}" TextWrapping="Wrap">If there is no default handler for .png or the <Bold>Launch Open With</Bold> button is clicked an 'Open With' dialog is displayed.</TextBlock>
        </StackPanel>
    </Grid>
</Page>