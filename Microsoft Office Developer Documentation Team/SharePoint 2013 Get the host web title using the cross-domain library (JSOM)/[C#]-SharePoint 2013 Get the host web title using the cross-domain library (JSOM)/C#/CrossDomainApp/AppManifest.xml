<?xml version="1.0" encoding="utf-8" ?>
<App xmlns="http://schemas.microsoft.com/sharepoint/2012/app/manifest"
     ProductID="{CF8A1E3B-5F13-450B-8784-26A0BC983DDD}"
     Version="1.0.1.0"
     SharePointMinVersion="15.0.0.0"
     Name="CrossDomainReadTitleJSOM"
>
  <Properties>
    <Title>CrossDomainReadTitleJSOM</Title>
    
    <!-- The {StandardTokens} token includes the 
         query string parameters required by the sample.  -->
    <StartPage>~appWebUrl/Pages/AppHost.aspx?{StandardTokens}</StartPage>
  </Properties>

<AppPrincipal>
    <AutoDeployedWebApplication />
  </AppPrincipal>

<AppPermissionRequests>
    <AppPermissionRequest 
        Scope="http://sharepoint/content/sitecollection/web" 
        Right="Read" />
</AppPermissionRequests>

</App>
