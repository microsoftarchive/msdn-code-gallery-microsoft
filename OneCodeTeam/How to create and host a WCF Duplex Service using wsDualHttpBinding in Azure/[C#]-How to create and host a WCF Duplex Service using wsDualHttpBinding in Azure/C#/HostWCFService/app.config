<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <trace>
            <listeners>
                <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
                    name="AzureDiagnostics">
                    <filter type="" />
                </add>
            </listeners>
        </trace>
    </system.diagnostics>
  <system.serviceModel>
    <services>

      <service name="ServiceCalculate.ServiceCalculate" behaviorConfiguration="CalculatorServiceBehavior" >
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost/CalculatorService"/>
          </baseAddresses>
        </host>
        <endpoint address="" binding="wsDualHttpBinding" contract="ServiceCalculate.IServiceCalculate"> </endpoint>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors >
        <behavior name="CalculatorServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>